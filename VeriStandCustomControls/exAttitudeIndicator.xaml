<UserControl x:Class="NationalInstruments.VeriStand.CustomControlsExamples.exAttitudeIndicator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:ni="http://schemas.ni.com/controls/2009/xaml/presentation"
             xmlns:local="clr-namespace:NationalInstruments.VeriStand.CustomControlsExamples"
             mc:Ignorable="d" 
             d:DesignHeight="275" d:DesignWidth="380
             ">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary
                    Source="/NationalInstruments.Controls.SkinProviders.Industry;component/IDESkins/AttitudeIndicator.IDE.WPF.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <ni:AttitudeIndicator x:Name="AttitudeIndicator" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Height="275" Width="380" />
</UserControl>
